prefix=@prefix@
exec_prefix=${prefix}
libdir=@libdir@
includedir=@includedir@
version=@ALLEGRO_VERSION@
suffix=@lib_type@@lib_linkage@
versuffix=@versuffix@

Name: allegro_olf
Description: Allegro game programming library, TrueType outline fonts addon
Version: ${version}
Libs: -L${libdir} -lallegro_olf${suffix}
Libs.private: @link_with@
Requires: allegro_font${suffix}-${versuffix} >= ${version} 
Cflags: -I${includedir}
